# Local Development Environment Configuration
# Copy this file to .env.local and update with your actual values

# API Tokens (Replace with your actual tokens)
JWT_TOKEN=your_jwt_token_here
AIRCALL_API_TOKEN=your_aircall_token_here
SLACK_API_TOKEN=your_slack_token_here

# Application Settings for Local Development
LOG_LEVEL=debug
NODE_ENV=development
TZ=America/Chicago
PORT=8081

# Database Configuration (if needed for local testing)
DATABASE_URL=postgresql://user:password@localhost:5432/database
DATABASE_PASSWORD=your_database_password_here

# Other Service Tokens (if needed)
REDIS_PASSWORD=your_redis_password_here
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Local Development Overrides
# These can be used to override production settings for local testing
LOCAL_TEST_MODE=true
ENABLE_DEBUG_LOGGING=true
